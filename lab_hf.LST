                 ;*** DEFINÍCIÓK ***
                 
                 ; ATMega 128 definíciós fájl betöltése
                 .list
                 
                 ; Regiszterkiosztás
                 .def	temp	= r16			; általános segédregiszter
                 .def	temp2	= r17			; általános segédregiszter
                 .def	sstate	= r18			; SW0 aktuális állapota
                 .def	sprev	= r19			; SW0 elõzõ állapota
                 .def	pos		= r20			; visszajátszás aktuális pozíciója
                 .def	tconst	= r21			; idõzítéshez használt regiszter
                 
                 ; SRAM foglalása
                 .dseg
000100           	pattern:	.byte	256		; Az eltárolt minták helye
000200           	count:		.byte	1		; Az eltárolt minták száma
                 
                 
                 
                 ;*** RESET ÉS IT VEKTORTÁBLA ***
                 
                 .cseg 
                 .org	0x0000		; Kódszegmens kezdõcíme 
                 
000000 940c 0046 jmp		main		; Reset vektor 
000002 940c 00d2 jmp		dummy		; EXTINT0 Handler
000004 940c 00d2 jmp		dummy		; EXTINT1 Handler
000006 940c 00d2 jmp		dummy		; EXTINT2 Handler
000008 940c 00d2 jmp		dummy		; EXTINT3 Handler
00000a 940c 00d2 jmp		dummy		; EXTINT4 Handler (INT gomb)
00000c 940c 00d2 jmp		dummy		; EXTINT5 Handler
00000e 940c 00d2 jmp		dummy		; EXTINT6 Handler
000010 940c 00d2 jmp		dummy		; EXTINT7 Handler
000012 940c 00d2 jmp		dummy		; Timer2 Compare Match Handler 
000014 940c 00d2 jmp		dummy		; Timer2 Overflow Handler 
000016 940c 00d2 jmp		dummy		; Timer1 Capture Event Handler 
000018 940c 00d2 jmp		dummy		; Timer1 Compare Match A Handler 
00001a 940c 00d2 jmp		dummy		; Timer1 Compare Match B Handler 
00001c 940c 00d2 jmp		dummy		; Timer1 Overflow Handler 
00001e 940c 0089 jmp		t0it		; Timer0 Compare Match Handler 
000020 940c 00d2 jmp		dummy		; Timer0 Overflow Handler 
000022 940c 00d2 jmp		dummy		; SPI Transfer Complete Handler 
000024 940c 00d2 jmp		dummy		; USART0 RX Complete Handler 
000026 940c 00d2 jmp		dummy		; USART0 Data Register Empty Hanlder 
000028 940c 00d2 jmp		dummy		; USART0 TX Complete Handler 
00002a 940c 00d2 jmp		dummy		; ADC Conversion Complete Handler 
00002c 940c 00d2 jmp		dummy		; EEPROM Ready Hanlder 
00002e 940c 00d2 jmp		dummy		; Analog Comparator Handler 
000030 940c 00d2 jmp		dummy		; Timer1 Compare Match C Handler 
000032 940c 00d2 jmp		dummy		; Timer3 Capture Event Handler 
000034 940c 00d2 jmp		dummy		; Timer3 Compare Match A Handler 
000036 940c 00d2 jmp		dummy		; Timer3 Compare Match B Handler 
000038 940c 00d2 jmp		dummy		; Timer3 Compare Match C Handler 
00003a 940c 00d2 jmp		dummy		; Timer3 Overflow Handler 
00003c 940c 00d2 jmp		dummy		; USART1 RX Complete Handler 
00003e 940c 00d2 jmp		dummy		; USART1 Data Register Empty Hanlder 
000040 940c 00d2 jmp		dummy		; USART1 TX Complete Handler 
000042 940c 00d2 jmp		dummy		; Two-wire Serial Interface Handler 
000044 940c 00d2 jmp		dummy		; Store Program Memory Ready Handler 
                 
                 
                 
                 ;*** FÕPROGRAM, INICIALIZÁCIÓK ***
                 
                 .org	0x0046
                 main:
                 
                 	; stack inicializálása
000046 ef0f      	ldi		temp,LOW(RAMEND)	; RAMEND = RAM végcíme
000047 bf0d      	out		SPL,temp			; (ld."m128def.inc") 
000048 e100      	ldi		temp,HIGH(RAMEND)
000049 bf0e      	out		SPH,temp
                 
                 	; LED0-7 inicializálása
00004a ef0f      	ldi		temp, 0xFF			; portbitek kimenetek
00004b bb04      	out		DDRC, temp			; PORTC kimenet
                 
                 	; SW0 inicializálása
00004c e000      	ldi		temp, 0x00			; portbitek bemenetek
00004d 9300 0064 	sts		DDRG, temp			; PORTG bemenet
00004f ef0f      	ldi		temp, 0xFF			; pull-up engedélyezve
000050 9300 0065 	sts		PORTG, temp			; PORTG bemenetein
                 
                 	; SW0 alapállapotának betöltése
000052 9130 0063 	lds		sprev, PING
000054 7031      	andi	sprev, 0x01			; PING LSB SW0 állapota 
                 
                 	; BTN0-2 inicializálása
000055 e000      	ldi		temp, 0x00			; portbitek bemenetek
000056 b902      	out		DDRE, temp			; PORTE bemenet
000057 ef0f      	ldi		temp, 0xFF			; pull-up engedélyezve
000058 b903      	out		PORTE, temp			; PORTE bemenetein
                 
                 	; BNT3 inicializálása
000059 e000      	ldi		temp, 0x00			; portbitek bemenetek
00005a bb07      	out		DDRB, temp			; PORTB bemenet
00005b ef0f      	ldi		temp, 0xFF			; pull-up engedélyezve
00005c bb08      	out		PORTB, temp			; PORTB bemenetein
                 
                 	; potméter inicializálása
00005d e000      	ldi		temp,0b00000000		; portbitek bemenetek
00005e 9300 0061 	sts		DDRF,temp			; PORTF bemenet
000060 e603      	ldi 	temp, 0b01100011 	; ADMUX: 5V ref, balra igazított, potméter
                 				  ; 01...... 	    REFS = 01 (referenciafeszültség: 5V VCC) 
                 				  ; ..1..... 	    ADLAR = 1 (balra igazított) 
                 				  ; ...00011 	    ADMUX = 00011 (potméter) 
000061 b907      	out ADMUX, temp 
000062 ee07      	ldi 	temp, 0b11100111 	; ADCSRA: folyamatos futás, IT, 128-as elõosztó 
                 				  ; 1....... 	    ADEN = 1 (A/D engedélyezése) 
                 				  ; .1...... 	    ADSC = 1 (start conversion) 
                 				  ; ..1..... 	    ADFR = 1 (free running / folyamatos konverzió) 
                 				  ; ...0.... 	    ADIF (nem töröljük a megszakításjelzõ flaget) 
                 				  ; ....0... 	    ADIE = 1 (megszakítások engedélyezése) 
                 				  ; .....111 	    ADPS = 111 (128-as elõosztó) 
000063 b906      	out ADCSRA, temp
                 	
                 	; SRAM-ban és regiszterben a számlálók kinullázása
000064 e040      	ldi		pos, 0x00
000065 9340 0200 	sts		count, pos
                 
                 
                 	; 10ms idõzítõ inicializálása
                 
000067 e159      	ldi		tconst, 25			; idõzítési konstans (T = 25*10 ms = 250ms)
000068 e00f      	ldi		temp, 0b00001111	; Timer 0 TCCR0 regiszter
                 				  ;	0.......	    FOC=0
                 				  ;	.0..1...		WGM=10 (CTC mod)
                 				  ;	..00....		COM=00 (kimenet tiltva)
                 				  ;	.....111		CS0=111 (CLK/1024)
000069 bf03      	out		TCCR0, temp
                 
00006a e60c      	ldi		temp, 108			; 11059200Hz/1024 = 108*100
00006b bf01      	out		OCR0, temp			; Timer 0 OCR0 regiszter
                 
00006c e002      	ldi		temp, 0b00000010	; Timer IT Mask regiszter
                 				  ; 000000..		Timer2,1 IT tiltva
                 				  ; ......1.		OCIE0=1
                 				  ; .......0		TOIE0=0
00006d bf07      	out		TIMSK, temp
                 
00006e 9478      	sei							; globális IT engedélyezve
                 
                 
                 
                 ;*** FÕPROGRAM, VÉGTELEN CIKLUS ***
                 		
00006f 9120 0063 loop:	lds		sstate,PING				; kapcsolók állaptának beolvasása
000071 7021      		andi	sstate, 0x01			; maszkolás -> csak sw0 állapota tárolódik, sstate LSB-jén
000072 2f02      		mov		temp, sstate			
000073 2703      		eor		temp, sprev				; jelen és elõzõ állapot összehasonlítása
000074 f419      		brne	sw0it					; ha nem egyezik a két állapot, kapcslás volt SW0-n -> kezeljük
                 		carry_on:
000075 2f32      			mov		sprev, sstate		; aktuális állapot betöltése a köv. ciklusban való összehasonlításhoz
000076 940c 006f 			jmp		loop				; végtelen hurok 
                 
                 
                 
                 ;*** SW0 INTERRUPT ***
                 
                 sw0it:
000078 e0a0      	ldi		XL, LOW(SRAM_START)		; pointer vissza az SRAM elejére
000079 e0b1      	ldi		XH, HIGH(SRAM_START)
00007a e000      	ldi		temp, 0x00
00007b bb05      	out		PORTC, temp				; LED-ek elsötétítése
00007c ff20      	sbrs	sstate, 0				; SW0 aktív ?
00007d 940c 0081 	jmp		rec_mode_it				; SW0 = 1, felvétel mód
00007f 940c 0086 	jmp		play_mode_it			; SW0 = 0, lejátszás mód
                 
                 rec_mode_it:						; 'interrupt', felvétel mód inicializálása
000081 e000      	ldi		temp, 0
000082 9300 0200 	sts		count, temp
000084 940c 0075 	jmp		carry_on				; visszatérés az 'interrupt'-ból
                 		
                 play_mode_it:						; 'interrupt', lejátszási mód inicializálása
000086 e040      	ldi		pos, 0
000087 940c 0075 	jmp		carry_on				; visszatérés az 'interrupt'-ból
                 
                 
                 
                 ;*** 10ms TIMER INTERRUPT ***
                 
                 .dseg
000201           	c_timer:	.byte	1		; c_timer számláló, 1 byte helyfoglalás RAM-ban 
                 
                 .cseg
                 
                 t0it:
000089 930f      	push	temp				; segédregiszter mentése
00008a b70f      	in		temp, SREG			; státusz mentése
00008b 930f      	push	temp
00008c 9100 0201 	lds		temp, c_timer		; c_timer számláló
00008e 950a      	dec		temp				; csökkentése
00008f 9300 0201 	sts		c_timer, temp		; és tárolása
000091 f5e9      	brne	t0ite				; ugrás, ha nem járt le
000092 2f05      	mov		temp, tconst		; számláló visszaállítása
000093 9300 0201 	sts		c_timer, temp
000095 ff20      	sbrs	sstate, 0			; SW0 aktív ?
000096 940c 009a 	jmp		rec					; SW0 = 1, felvétel mód
000098 940c 00bb 	jmp		play				; SW0 = 0, lejátszás mód
                 
                 
                 ; FELVÉTEL MÓD
                 rec:
00009a e159      	ldi		tconst, 25			; idõzítés rendbe szedése
                 
                 	; gombok beolvasása
00009b b101      	in 		temp, PINE			; BTN0-2
00009c b316      	in 		temp2, PINB			; BTN3
                 
                 	; Az 5-7 biteket átpakoljuk a 0-2 helyekre
00009d fb07      	bst		temp, 7
00009e f902      	bld 	temp, 2
00009f fb06      	bst		temp, 6
0000a0 f901      	bld		temp, 1
0000a1 fb05      	bst		temp, 5
0000a2 f900      	bld		temp, 0
                 
0000a3 fb17      	bst 	temp2, 7			; BTN3 értéke a transzfer regiszterbe
0000a4 f903      	bld		temp, 3				; visszatöltés a jó helyre (temp-ben most BTN0-3 bent van)
                 
                 	; negáljuk a gombok állapotát, mert lenyomva 0, de a LED 1-es állapotban világít
0000a5 ef1f      	ldi		temp2, 0xFF
0000a6 2701      	eor		temp, temp2
0000a7 700f      	andi 	temp, 0x0F
                 
0000a8 9110 0200 	lds		temp2, count		; számláló növelése
0000aa 9513      	inc 	temp2
                 
0000ab f061      	breq	rec_overflow		; túlcsordulás kezelése
                 
0000ac 9310 0200 	sts		count, temp2		; számláló visszaírása
0000ae 930d      	st		X+, temp			; aktuális minta tárolása SRAM-ban
0000af 930f      	push	temp				; temp tárolása a stack-en
                 
                 	; jobb oszlopban lévõ LED-ek villogtatása
0000b0 b315      	in		temp2,PORTC			; LED állapot beolvasása
0000b1 ef0f      	ldi		temp, 0xFF			; negálás, a villogtatáshoz
0000b2 2710      	eor		temp2, temp
0000b3 7f10      	andi	temp2, 0xF0
                 
0000b4 910f      	pop		temp				; temp visszaolvasása a stack-rõl
0000b5 2b01      	or		temp, temp2			; bal oszlopban az aktuális gombminta, jobb oszlop villog
0000b6 940c 00ce 	jmp		leds
                 
                 ; felvétel mód túlcsordulásának kezelése
                 rec_overflow:
0000b8 ef0f      	ldi		temp, 0xFF 			; minden LED világít
0000b9 940c 00ce 	jmp	 	leds
                 
                 
                 ; LEJÁTSZÁS MÓD
                 play:
0000bb b105      	in 		temp, ADCH			; potméter beolvasása
                 
                 	; 8 állás meghatározása
0000bc 7e00      	andi	temp, 0xE0			; csak a 3 felsõ bit érdekes
0000bd 9506      	lsr		temp				; 4-gyel osztás, idõzítés kezeléséhez
0000be 9506      	lsr 		temp
0000bf 9503      	inc		temp
0000c0 2f50      	mov		tconst, temp		; tconst beállítása arra az idõzítésre, amit a potméter mutat
                 
                 	; aktuális mentett minta visszaolvasása
0000c1 910d      	ld		temp, X+
0000c2 9543      	inc 	pos
                 
                 	; ha körbeértünk, újrakezdjük
0000c3 9110 0200 	lds		temp2, count
0000c5 1b14      	sub		temp2, pos
0000c6 f011      	breq	play_loop
                 
0000c7 940c 00ce 	jmp 	leds
                 
                 
                 ; lejátszás végén ismétlés
                 play_loop:
0000c9 e0a0      	ldi		XL, LOW(SRAM_START)		; pointer vissza az SRAM elejére
0000ca e0b1      	ldi		XH, HIGH(SRAM_START)
0000cb e040      	ldi		pos, 0					; pozíció regiszter nullázása
0000cc 940c 00ce 	jmp		leds
                 
                 
                 ; LED-sor a regiszterbõl a kimenetre
                 leds:
0000ce bb05      	out		PORTC, temp
                 
                 
                 ; Timer IT vége
                 t0ite:
0000cf 910f      	pop		temp					; regiszterek visszaállítása
0000d0 bf0f      	out		SREG, temp
0000d1 910f      	pop		temp
                 
                 ; Visszatérés az IT-ból
                 dummy:


